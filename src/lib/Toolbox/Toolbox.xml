<xml>
    <category name="Sprites" colour="#4C97FF">
        <block type="motion_goto_position">
            <value name="X">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="Y">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
        </block>
        <block type="motion_changeby_position">
            <value name="X">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
            <value name="Y">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="motion_getsprite">
            <value name="SPRITE">
                <shadow type="text">
                    <field name="TEXT">Sprite1</field>
                </shadow>
            </value>
        </block>
        <block type="motion_getxof" />
        <block type="motion_getyof" />
        <sep gap="48"></sep>
        <block type="motion_setdirection">
            <value name="DIR">
                <shadow type="math_angle">
                    <field name="NUM">90</field>
                </shadow>
            </value>
        </block>
        <block type="motion_getdirof" />
        <block type="motion_setrotatestyle" />
        <block type="motion_getrotatestyleof" />
        <sep gap="48"></sep>
        <block type="motion_stage" />
        <block type="motion_isstage" />
    </category>
    <category name="Sound" colour="#CF63CF">
        <!--<block type="sound_playsound" />-->
        <block type="sound_startsound" />
        <!--<sep gap="48"></sep>-->
        <!--<block type="sound_stopallsounds" />-->
        <block type="sound_getvolumeof" />
        <sep gap="48"></sep>
        <block type="sound_setprojectvolume">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">100</field>
                </shadow>
            </value>
        </block>
        <block type="sound_changeprojectvolume">
            <value name="VALUE">
                <shadow type="math_number">
                    <field name="NUM">-10</field>
                </shadow>
            </value>
        </block>
        <block type="sound_getprojectvolume" />
    </category>
    <category name="Events" colour="#FFBF00">
        <block type="events_interval">
            <value name="TIME">
                <shadow type="math_positive_number">
                    <field name="NUM">2</field>
                </shadow>
            </value>
        </block>
        <block type="events_timeout">
            <value name="TIME">
                <shadow type="math_positive_number">
                    <field name="NUM">2</field>
                </shadow>
            </value>
        </block>
        <sep gap="48"></sep>
        <block type="events_always" />
    </category>
    <category name="Control" colour="#FFAB19">
        <block type="control_wait">
            <value name="MS">
                <shadow type="math_positive_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="control_waituntil" />
        <sep gap="48"></sep>
        <block type="control_repeat">
            <value name="TIMES">
                <shadow type="math_whole_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="control_forever" />
        <sep gap="48"></sep>
        <block type="control_if" />
        <block type="control_if">
            <mutation else="1" />
        </block>
        <block type="control_ifthenreturn">
            <value name="X">
                <shadow type="text">
                    <field name="TEXT">foo</field>
                </shadow>
            </value>
            <value name="Y">
                <shadow type="text">
                    <field name="TEXT">bar</field>
                </shadow>
            </value>
        </block>
        <sep gap="48"></sep>
        <block type="control_switch" />
        <block type="control_case">
            <value name="VALUE">
                <shadow type="text">
                    <field name="TEXT">foo</field>
                </shadow>
            </value>
        </block>
        <block type="control_default" />
        <block type="control_break" />
        <sep gap="48"></sep>
        <block type="control_while" />
        <sep gap="48"></sep>
        <block type="control_try_catch" />
        <block type="control_throwerror">
            <value name="VALUE">
                <shadow type="text">
                    <field name="TEXT">Hello!</field>
                </shadow>
            </value>
        </block>
        <!--<block type="control_error" />-->
    </category>
    <category name="Sensing" colour="#5CB1D6">
        <block type="sensing_keypress" />
        <sep gap="48"></sep>
        <block type="sensing_alert">
            <value name="ALERT">
                <shadow type="text">
                    <field name="TEXT">Hello!</field>
                </shadow>
            </value>
        </block>
        <block type="sensing_prompt">
            <value name="ALERT">
                <shadow type="text">
                    <field name="TEXT">How are you?</field>
                </shadow>
            </value>
        </block>
        <block type="sensing_confirm">
            <value name="ALERT">
                <shadow type="text">
                    <field name="TEXT">Are you sure?</field>
                </shadow>
            </value>
        </block>
        <sep gap="48"></sep>
        <block type="sensing_time" />
        <block type="sensing_currenttime" />
        <block type="sensing_leapyear" />
        <sep gap="48"></sep>
        <block type="sensing_isfocused" />
        <sep gap="48"></sep>
        <block type="sensing_getcilentmouseposition" />
        <block type="sensing_getmouseposition" />
        <block type="sensing_ismousedown" />
        <sep gap="48"></sep>
        <block type="sensing_gettimer" />
        <block type="sensing_resettimer" />
        <block type="sensing_pausetimer" />
        <block type="sensing_resumetimer" />
        <!--<block type="sensing_optiontimer" />-->
        <sep gap="48"></sep>
        <block type="sensing_getusername" />
    </category>
    <category name="Literals" colour="#59C08E">
        <block type="literals_null" />
        <block type="literals_true" />
        <block type="literals_false" />
        <block type="literals_randomBoolean" />
        <block type="literals_number" />
        <block type="literals_direction" />
        <block type="literals_string" />
        <block type="literals_color" />
        <sep gap="48"></sep>
        <block type="literals_newline" />
        <block type="literals_tabchar" />
        <sep gap="48"></sep>
        <block type="literals_array" />
        <block type="literals_arraylength" />
        <block type="literals_object" />
    </category>
