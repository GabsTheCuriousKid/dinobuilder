            <value name="Y">
                <shadow type="math_number">
                    <field name="NUM"></field>
                </shadow>
            </value>
        </block>
        <block type="operators_adv">
            <value name="Y">
                <shadow type="math_number">
                    <field name="NUM"></field>
                </shadow>
            </value>
