    <sep></sep>
    <category name="Blocks" colour="#FF6680">
        <block type="blocks_create">
            <value name="CONDITION">
                <block type="literals_false" />
            </value>
        </block>
        <!--<block type="blocks_createcap" />-->
        <block type="blocks_createobject">
            <value name="CONDITION">
